@Library('script@main')_
pipeline {
       agent any
       stages{        
        stage("Checkout Csv file repository") {
                               steps {
                                        git branch: 'main', url: 'https://github.com/AbhiJag29/file.git'
                                    }
                                }
        stage("Reading file")
        {
            steps
            {
                  script{
                      readFilecode 'C:/Windows/System32/config/systemprofile/AppData/Local/Jenkins/.jenkins/workspace/CaseStudy/Hello.csv'
                      inputFile.fileRead("Hello.csv")
                      }
            }
        }
       }
       post {
        always {
           // emailext body: '${env.BUILD_URL} has result ${currentBuild.result}', subject: 'test email', to: 'abhiraj.2998@gmail.com'
            mail to: 'abhiraj.2998@gmail.com',
          subject: "Status of pipeline: ${currentBuild.fullDisplayName}",
          body: "${env.BUILD_URL} has result ${currentBuild.result}"
        }
    }
}
